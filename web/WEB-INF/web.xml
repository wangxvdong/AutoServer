<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>testServlet</servlet-name>
        <servlet-class>ServletOfPage.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testServlet</servlet-name>
        <url-pattern>/ServletOfPage/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>servletatindex</servlet-name>
        <servlet-class>ServletOfPage.ServletAtIndex</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletatindex</servlet-name>
        <url-pattern>/ServletOfPage/ServletAtIndex</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registerAtVip</servlet-name>
        <servlet-class>ServletOfPage.RegisterOfVip</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerAtVip</servlet-name>
        <url-pattern>/ServletOfPage/RegisterOfVip</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>servletoflogin</servlet-name>
        <servlet-class>ServletOfPage.ServletOfLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletoflogin</servlet-name>
        <url-pattern>/ServletOfPage/ServletOfLogin</url-pattern>
    </servlet-mapping>
<listener>
    <listener-class>Global.MySessionListener</listener-class>
</listener>
</web-app>